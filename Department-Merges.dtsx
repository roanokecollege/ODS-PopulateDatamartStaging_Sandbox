          SQLTask:BypassPrepare="False"
          SQLTask:SqlStatementSource="MERGE INTO Datamart.dbo.Department as TARGET&#xA;USING (SELECT dept_id, dept_description FROM Staging.dbo.dm_departments) as SOURCE&#xA;ON TARGET.Code = SOURCE.dept_id&#xA;WHEN MATCHED THEN &#xA;&#x9;UPDATE SET Target.code = Source.dept_id, Target.Name = Source.dept_description&#xA;WHEN NOT MATCHED THEN&#xA;&#x9;INSERT (Name, Code) VALUES (SOURCE.dept_description, Source.dept_id)&#xA; ;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
