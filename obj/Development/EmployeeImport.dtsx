          SQLTask:SqlStatementSource="truncate table Datamart.dbo.PersonEmployee_department_map&#xA;&#xA;MERGE Datamart.dbo.PersonEmployee_Department_Map AS Target&#xA;USING (SELECT RCID, &#xA;&#x9;(SELECT key_DepartmentId FROM Datamart.dbo.Department &#xA;&#x9;  WHERE rtrim(name) like rtrim(department)) as DeptKey, DeptChair&#xA; FROM Staging.dbo.DM_Employees WHERE len(rtrim(department)) &gt; 0 AND Not (Department is null)) AS Source&#xA;ON (Target.rcid = Source.rcid)&#xA;WHEN NOT MATCHED BY TARGET THEN&#xA;    INSERT (rcid, fkey_DepartmentId,Rank,Chair)&#xA;    VALUES (Source.rcid, Source.DeptKey, 1, Source.DeptChair);&#xA;    &#xA;&#xA;MERGE    Datamart.dbo.personemployee_department_map as target&#xA;using (select rcid, (SELECT key_DepartmentId FROM Datamart.dbo.Department &#xA;&#x9;  WHERE rtrim(name) like rtrim(AdditionalDepartment)) as DeptKey&#xA;&#x9; From Staging.dbo.DM_Employees WHERE len(rtrim(AdditionalDepartment)) &gt; 0 AND NOT (AdditionalDepartment is null)AND NOT (AdditionalDepartment = 'other')) as source&#xA;ON ((Target.rcid = Source.rcid) AND (target.fkey_departmentid = Source.deptkey))&#xA;WHEN Not Matched By Target THEN&#xA;&#x9; INSERT (rcid, fkey_DepartmentId,Rank)&#xA;&#x9; VALUES (Source.rcid, Source.DeptKey, 2);&#xA;&#xA;&#xA;MERGE  Datamart.dbo.personemployee_department_map as target&#xA;using (select rcid, (SELECT key_DepartmentId FROM Datamart.dbo.Department &#xA;&#x9;  WHERE rtrim(name) like rtrim(Sec1Dept)) as DeptKey&#xA;&#x9; From Staging.dbo.DM_Employees WHERE len(rtrim(Sec1Dept)) &gt; 0 AND NOT (Sec1Dept is null)) as source&#xA;ON ((Target.rcid = Source.rcid) AND (target.fkey_departmentid = Source.deptkey))&#xA;WHEN Not Matched By Target THEN&#xA;&#x9; INSERT (rcid, fkey_DepartmentId,Rank,secretary)&#xA;&#x9; VALUES (Source.rcid, Source.DeptKey, 1, 'Y')&#xA;WHEN Matched THEN&#xA;&#x9;UPDATE set secretary= 'Y';&#xA;&#x9;&#xA;MERGE  Datamart.dbo.personemployee_department_map as target&#xA;using (select rcid, (SELECT key_DepartmentId FROM Datamart.dbo.Department &#xA;&#x9;  WHERE rtrim(name) like rtrim(Sec2Dept)) as DeptKey&#xA;&#x9; From Staging.dbo.DM_Employees WHERE len(rtrim(Sec2Dept)) &gt; 0 AND NOT (Sec2Dept is null)) as source&#xA;ON ((Target.rcid = Source.rcid) AND (target.fkey_departmentid = Source.deptkey))&#xA;WHEN Not Matched By Target THEN&#xA;&#x9; INSERT (rcid, fkey_DepartmentId,Rank,secretary)&#xA;&#x9; VALUES (Source.rcid, Source.DeptKey, 2, 'Y')&#xA;WHEN Matched THEN&#xA;&#x9;UPDATE set secretary= 'Y';&#x9;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
