          SQLTask:SqlStatementSource="&#xA;DECLARE @keyEmailType as integer&#xA;&#xA;SELECT @keyEmailType = key_emailTypeid&#xA;FROM DataMart.dbo.EmailType&#xA;WHERE Description = 'Campus'&#xA;&#xA;&#xA;MERGE Datamart.dbo.Email AS Target&#xA;USING (SELECT RCID&#xA; FROM Staging.dbo.DM_Employees) as Source&#xA;ON (Target.rcid = Source.rcid and Target.fkey_EmailTypeId = @keyEmailType)&#xA;WHEN MATCHED THEN&#xA;   DELETE;&#x9;  &#xA;&#xA;MERGE Datamart.dbo.Email AS Target&#xA;USING (SELECT RCID, Email&#xA; FROM Staging.dbo.DM_Employees WHERE len(rtrim(email)) &gt; 0) AS Source&#xA;ON (Target.rcid = Source.rcid and fkey_EmailTypeId = @keyEmailType)&#xA;WHEN MATCHED THEN&#xA;&#x9;UPDATE SET TARGET.EmailAddress = Source.Email&#xA;WHEN NOT MATCHED THEN&#xA;    INSERT (rcid, EmailAddress, fkey_EmailTypeId)&#xA;    VALUES (Source.rcid, Source.Email, @keyEmailType);" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
