      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{3397EB41-38A9-4D42-A673-809835054D8D}"
          SQLTask:BypassPrepare="False"
          SQLTask:SqlStatementSource="MERGE Datamart.dbo.Buildings AS Target&#xA;USING (SELECT Buildings_id, Buildings_DESC as Name, BLDG_IMAGE_RESOURCE, BLDG_LATITUDE, BLDG_LONGITUDE, CURRENT_TIMESTAMP as ImportDate , BLDG_ADDRESS, BLDG_CITY, BLDG_ZIP, BLDG_TYPE, BLDG_CONDITION, date&#xA;FROM Staging.dbo.DM_Buildings) AS Source&#xA;ON (Target.Buildings_id = Source.Buildings_id)&#xA;WHEN MATCHED THEN&#xA;    UPDATE SET Target.BuildingName = Source.Name, Target.BUILDING_IMAGE_RESOURCE = Source.BLDG_IMAGE_RESOURCE, Target.BUILDING_LATITUDE = Source.BLDG_LATITUDE, Target.BuildingType = Source.BLDG_TYPE,&#xA;    Target.BUILDING_LONGITUDE = SOURCE.BLDG_LONGITUDE, Target.BLDG_ADDRESS = SOURCE.BLDG_ADDRESS, Target.BLDG_CITY = SOURCE.BLDG_CITY, Target.BLDG_ZIP = SOURCE.BLDG_ZIP, Target.BLDG_CONDITION = Source.BLDG_CONDITION, Target.date = SOURCE.date, Target.updated_at = getdate() &#xA;WHEN NOT MATCHED BY TARGET THEN&#xA;    INSERT (Buildings_id, BuildingName, BuildingType, Building_image_resource, building_longitude, building_latitude, BLDG_ADDRESS, BLDG_CITY, BLDG_ZIP, BLDG_CONDITION, date,updated_at)&#xA;    VALUES (Source.Buildings_id, Source.Name,Source.BLDG_TYPE, Source.BLDG_IMAGE_RESOURCE, Source.BLDG_LATITUDE, Source.BLDG_LONGITUDE, Source.BLDG_ADDRESS, Source.BLDG_CITY, Source.BLDG_ZIP,Source.BLDG_CONDITION, Source.date, CONVERT(varchar(255), GETDATE(), 120));&#xA;    &#xA;   " xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
