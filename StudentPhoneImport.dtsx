      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{9C4CC6C3-C05F-4752-9316-F345F9886E51}"
          SQLTask:SqlStatementSource="DECLARE @keyPhoneType as int&#xA;&#xA;SELECT @KeyPhoneType = key_PhoneTypeId&#xA;FROM DataMart.dbo.PhoneType&#xA;Where Description = 'Home'&#xA;&#xA;&#xA;MERGE Datamart.dbo.Phone AS Target&#xA;USING (SELECT RCID&#xA; FROM Staging.dbo.DM_Student) AS Source&#xA;ON (Target.rcid = Source.rcid and Target.fkey_PhoneTypeId = @keyPhoneType)&#xA;WHEN MATCHED THEN&#xA;   Delete;&#xA;&#xA;&#xA;&#xA;MERGE Datamart.dbo.Phone AS Target&#xA;USING (SELECT RCID, HomePhone&#xA; FROM Staging.dbo.DM_Student Where HomePhone is not null  AND len(rtrim(HomePhone)) &gt; 0) AS Source&#xA;ON (Target.rcid = Source.rcid and Target.fkey_PhoneTypeId = @keyPhoneType)&#xA;WHEN MATCHED THEN&#xA;    UPDATE SET Target.PhoneNumber = Source.HomePhone&#xA;WHEN NOT MATCHED BY TARGET THEN&#xA;    INSERT (rcid, PhoneNumber, fkey_phoneTypeId)&#xA;    VALUES (Source.rcid, Source.HomePhone, @KeyPhoneType);" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
