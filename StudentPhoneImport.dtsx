    <DTS:Executable
      DTS:refId="Package\Merge Cell Phone Numbers"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Execute SQL Task"
      DTS:DTSID="{4E633675-CCE1-4C51-B9D0-33AFCD80DA94}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="Merge Cell Phone Numbers"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; Microsoft SQL Server 2008; © 2007 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{9C4CC6C3-C05F-4752-9316-F345F9886E51}"
          SQLTask:SqlStatementSource="DECLARE @keyPhoneType as int&#xA;&#xA;SELECT @KeyPhoneType = key_PhoneTypeId&#xA;FROM DataMart.dbo.PhoneType&#xA;Where Description = 'Cell'&#xA;&#xA;&#xA;MERGE Datamart.dbo.Phone AS Target&#xA;USING (SELECT RCID&#xA; FROM Staging.dbo.DM_Student) AS Source&#xA;ON (Target.rcid = Source.rcid and Target.fkey_PhoneTypeId = @keyPhoneType)&#xA;WHEN MATCHED THEN&#xA;   Delete;&#xA;&#xA;&#xA;&#xA;&#xA;MERGE Datamart.dbo.Phone AS Target&#xA;USING (SELECT RCID, CellPhone&#xA; FROM Staging.dbo.DM_Student where cellphone is not null AND len(rtrim(CellPhone)) &gt; 0)AS Source&#xA;ON (Target.rcid = Source.rcid and Target.fkey_PhoneTypeId = @keyPhoneType)&#xA;&#xA;WHEN MATCHED THEN&#xA;    UPDATE SET Target.PhoneNumber = Source.CellPhone&#xA;&#xA;WHEN NOT MATCHED BY TARGET THEN&#xA;    INSERT (rcid, PhoneNumber, fkey_phoneTypeId)&#xA;    VALUES (Source.rcid, Source.CellPhone, @KeyPhoneType);&#xA;   &#xA;    " xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
    <DTS:Executable
